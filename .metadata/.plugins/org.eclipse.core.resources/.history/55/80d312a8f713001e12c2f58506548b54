<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>

<script  src="http://code.jquery.com/jquery-latest.min.js"></script>

<script>
function fn_process(){
	
	let _id=$('t_id').val();
	if(_id=''){
		alert("ID 입력 하세요");
		return;
	}
	
	
	$.ajax({
		
		 type:"post",
		 async:true,
		 url:"http://localhost:8090/pro16/mem",
		 dataType:"text",
		 data:{id : _id},
		 success : function(info, textStatus){
			
		 },
 		 error : function(info, textStatus){
 			console.log(textStatus);
 			alert("에러가 발생했습니다.");ㅣ
 			
		 },
 		 complete : function(info, textStatus){
 			console.log(textStatus);
		 },
		
	});
}
	
</script>


</head>
<body>


<input type="text" id="t_id">
<input type="button" id="btnDuplicate" value="ID 중복체크하기" onClick="fn_process()" /><br><br>
   <div id="message"></div>

</body>
</html>